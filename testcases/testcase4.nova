fn genSum(int n) :: int {
    var int count = n;
    var int sum = 0;
    loop through (count > 0) {
        sum = sum + count;
        count = count - 1;
    };
    return : sum;
};

if (true) then {
    var int sum = genSum(7);
    println(sum);
};
else then {
    println("nothing over here");
};
